   "source": [
    "def print_fib(a1, a2, lim):\n",
    "    result = 0\n",
    "    while a1 + a2 <= lim:\n",
    "        result = a1 + a2\n",
    "        a1 = a2\n",
    "        a2 = result\n",
    "    return(result)"
   ]
  },
